<%- include("partials/header"); -%>
<div class="registerContainer">

  <form action="/register" method="post">

    <div class="form-group">
      <label class="whiteLabel" for="lastNameInput">Username</label>
      <input type="text" class="form-control" id="usernameInput" placeholder="Username" name="username">
    </div>
    <div class="form-group">
      <label class="whiteLabel" for="exampleInputEmail1">Email address</label>
      <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
    </div>
    <div class="form-group">
      <label class="whiteLabel" for="exampleInputPassword1">Password</label>
      <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password" name="password">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>

  </form>

</div>

<%- include("partials/footer"); -%>
